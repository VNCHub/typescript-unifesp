  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "build:frontend": "esbuild src/frontend/* --bundle --outdir=public/assets --sourcemap",
    "watch:frontend": "esbuild src/frontend/* --bundle --outdir=public/assets --sourcemap --watch",
    "build": "npm run build:frontend && tsc",
    "start": "node dist/server.js",
    "dev": "concurrently \"ts-node-dev --respawn --transpile-only src/server.ts\" \"npm run watch:frontend\""
  },
  "dependencies": {
    "@libsql/client": "^0.15.15",
    "@prisma/adapter-libsql": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "bcryptjs": "^3.0.3",
    "dotenv": "^17.2.3",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.1",
    "concurrently": "^9.2.1",
    "esbuild": "^0.27.0",
    "prisma": "^7.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
